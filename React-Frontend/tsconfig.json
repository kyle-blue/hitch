{
    "compilerOptions": {
        "module": "esnext",
        "noImplicitAny": false,
        "preserveConstEnums": true,
        "noEmit": true,
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "jsx": "react",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "downlevelIteration": true
        // "types": ["./src/typings/types.d.ts"]
    },
    "exclude": [
        "node_modules/**/*"
    ],
    "include": [
        "src/**/*"
    ]
}