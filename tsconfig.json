{
    "compilerOptions": {
        "module": "esnext",
        "noImplicitAny": false,
        "preserveConstEnums": true,
        "noEmit": true,
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "jsx": "react",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "downlevelIteration": true,
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/**/*"
    ]
}